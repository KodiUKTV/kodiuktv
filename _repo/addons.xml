<?xml version="1.0" encoding="UTF-8"?>
<addons>
<addon id="plugin.program.cape" name="Cape Breton Wizard" version="1.0" provider-name="KODIUK.TV">
  <requires>
    <import addon="xbmc.python" version="2.1.0"/>
  </requires>
  <extension point="xbmc.python.pluginsource" library="default.py">
        <provides>executable</provides>
  </extension>
  <extension point="xbmc.addon.metadata">
    <summary lang="en">Cape Breton android boxes official wizard</summary>
    <description lang="en">Cape Breton android boxes official wizard</description>
