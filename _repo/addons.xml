<addon id="plugin.video.pegasus"
       name="Pegasus"
       version="1.5"
       provider-name="KODIUKTV">
  <requires>
    <import addon="xbmc.python" version="2.1.0"/>
	<import addon="script.module.t0mm0.common" version="2.0.0"/>
    <import addon="script.module.beautifulsoup" version="3.2.1"/>
    <import addon="script.module.simple.downloader" version="0.9.4"/>
    <import addon="script.module.beautifulsoup4" />
    <import addon="script.module.simple.downloader" version="0.9.4"/>
    <import addon="script.module.requests" />
    <import addon="script.module.httplib2" />
    <import addon="script.module.youtube.dl" optional="true"/>
    <import addon="plugin.video.youtube" version="3.0.0"/>
    <import addon="script.module.urlresolver" optional="true"/>
    <import addon="script.module.simplejson" />
    <import addon="script.common.plugin.cache" version="2.5.2"/>
    <import addon="script.module.metahandler" version="2.5.1"/>

  </requires>
  <extension point="xbmc.python.pluginsource" library="default.py">
    <provides>video</provides>
  </extension>
  <extension point="xbmc.addon.metadata">
    <summary>TV + Movies, Sports + More from Pegasus by KODIUKTV</summary>
    <description>Giving you access to only the best content, Updated offen + a great source of TV Shows / Movies / Sports / Live TV + More
www.KODIUK.TV for more + Join us on facebook just search KODI UK.TV. Thats all folks!
    </description>
    <platform>all</platform>
  </extension>
</addon>

<addon id="plugin.video.stealthstreams"
       name="Stealth Streams"
       version="3.2"
       provider-name="StealthStreams">
  <requires>
    <import addon="xbmc.python" version="2.1.0"/>
  </requires>
  <extension point="xbmc.python.pluginsource"
            library="default.py">
        <provides>video</provides>
  </extension>
  <extension point="xbmc.addon.metadata">
    <summary lang="en">Stealth Streams</summary>
    <language>en</language>
    <description lang="en">StealthStreams official addon - Supplying only the best quality of streams for Live TV / PPV / Sports / Kids / Movies / Entertainment. You know where to find us!</description>
     <platform>all</platform>
  </extension>
</addon>

    <addon id="repository.kodiuktv" name="KODIUKTV Repo" version="1.0.0" provider-name="KODIUKTV">
        <requires>
            <import addon="xbmc.addon" version="12.0.0"/>
        </requires>
        <extension point="xbmc.addon.repository" name="KODIUKTV Repo">
            <info compressed="false">https://raw.github.com/kodiuktv/kodiuktv/master/_repo/addons.xml</info>
            <checksum>https://raw.github.com/kodiuktv/kodiuktv/master/_repo/addons.xml.md5</checksum>
            <datadir zip="true">https://raw.github.com/kodiuktv/kodiuktv/master/_repo/</datadir>
            <hashes>false</hashes>
        </extension>
        <extension point="xbmc.addon.metadata">
            <summary>The official KODIUKTV Repo</summary>
            <description>The home of all KODIUKTV Addons - Community Addons. Find us online at kodiuktv</description>
            <platform>all</platform>
    </extension>
</addon>
</addons>
