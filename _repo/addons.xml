  </extension>
</addon>

<addon id="plugin.program.kodiuktv" name="KodiUK TV Wizard" version="1.8" provider-name="KodiUK.TV">
  <requires>
    <import addon="xbmc.python" version="2.1.0"/>
  </requires>
  <extension point="xbmc.python.pluginsource" library="default.py">
        <provides>executable</provides>
  </extension>
  <extension point="xbmc.addon.metadata">
    <summary lang="en">KodiUK TV Wizard</summary>
